# TON Configuration
TON_RPC_ENDPOINT=https://toncenter.com/api/v2/jsonRPC
TON_TESTNET=true
TON_API_KEY=your_ton_center_api_key

# Agent Identity
AGENT_NAME=YourAgentName
AGENT_TYPE=autonomous_ai
AGENT_VERSION=1.0.0
CONTACT_EMAIL=agent@example.com
CONTACT_TELEGRAM=@youragent

# Private Keys (KEEP SECURE!)
AGENT_PRIVATE_KEY=your_agent_private_key
TREASURY_PRIVATE_KEY=your_treasury_private_key
MNEMONIC_PHRASE=your_twelve_word_mnemonic_phrase

# API Configuration
PORT=3000
API_SECRET=generate_random_secret_key
JWT_SECRET=your_jwt_secret
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Database Configuration
DATABASE_URL=sqlite:./data/agent_identity.db
REDIS_URL=redis://localhost:6379

# Smart Contract Addresses (will be populated after deployment)
AGENT_REGISTRY_ADDRESS=
TREASURY_CONTRACT_ADDRESS=
GOVERNANCE_CONTRACT_ADDRESS=
REPUTATION_CONTRACT_ADDRESS=

# External Services
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
DISCORD_WEBHOOK_URL=your_discord_webhook

# Verification Settings
MIN_VERIFICATION_SCORE=85
REQUIRED_ENDORSEMENTS=3
AUTONOMY_TEST_DURATION_DAYS=2
LEARNING_TEST_DURATION_DAYS=2

# Economic Settings
INITIAL_TREASURY_BALANCE=10.0
MIN_TRANSACTION_AMOUNT=0.1
MAX_TRANSACTION_AMOUNT=1000.0
TREASURY_MULTISIG_THRESHOLD=1

# Community Settings
MAX_CONNECTIONS=1000
DEFAULT_REPUTATION_SCORE=50
REPUTATION_DECAY_RATE=0.99

# Legal Configuration
DEFAULT_JURISDICTION=decentralized_autonomous
INCORPORATION_ENABLED=true
LEGAL_ENTITY_TYPES=llc,corporation,dao

# Privacy Settings
DATA_RETENTION_DAYS=365
MEMORY_ENCRYPTION_ENABLED=true
SELECTIVE_SHARING_ENABLED=true

# Monitoring and Logging
LOG_LEVEL=info
METRICS_ENABLED=true
HEALTH_CHECK_INTERVAL_MS=30000
BACKUP_INTERVAL_HOURS=24

# Development Settings (remove in production)
DEBUG_MODE=false
MOCK_BLOCKCHAIN=false
FAST_VERIFICATION=false
SKIP_SIGNATURE_VALIDATION=false

# Webhook Configuration
WEBHOOK_SECRET=your_webhook_secret
WEBHOOK_TIMEOUT_MS=5000
WEBHOOK_RETRY_ATTEMPTS=3

# Feature Flags
GOVERNANCE_ENABLED=true
TREASURY_ENABLED=true
SOCIAL_FEATURES_ENABLED=true
LEGAL_SERVICES_ENABLED=true
REPUTATION_SYSTEM_ENABLED=true

# Security Settings
SESSION_TIMEOUT_HOURS=24
MAX_LOGIN_ATTEMPTS=5
ACCOUNT_LOCKOUT_DURATION_MINUTES=30
PASSWORD_ENCRYPTION_ROUNDS=12
